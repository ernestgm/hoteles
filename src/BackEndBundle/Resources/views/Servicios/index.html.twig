<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Servicios</h5>

                    {#<div ibox-tools></div>#}
                </div>
                <div class="ibox-content">
                    {% verbatim %}
                        <table ng-controller="ServiciosController as se" ng-table="se.tableParams" show-filter="true" class="table table-striped table-bordered table-hover">
                           <tr ng-repeat="servicios in $data">
                               <td data-title="'Id'" sortable="'id'">{{ servicios.id }}</td>
                                <td data-title="'Descripcion'">{{ servicios.descripcion }}</td>
                                <td data-title="'Imagen'">{{ servicios.imagen }}</td>
                                <td data-title="'Horario'">{{ servicios.horario }}</td>
                                <td data-title="'Dias Habiles'">{{ servicios.diasHabiles }}</td>
                                <td data-title="'Orden'">{{ servicios.orden }}</td>
                               <td data-title="'Actions'" >
                                        <ul>
                                            <li>
                                                <a href="">show</a>
                                            </li>
                                            <li>
                                                <a href="">edit</a>
                                            </li>
                                        </ul>
                               </td>
                            </tr>
                        </table>
                    {% endverbatim %}
                </div>
            </div>
        </div>
    </div>
</div>




