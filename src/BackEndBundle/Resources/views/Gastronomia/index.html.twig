<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Gastronomia</h5>

                    {#<div ibox-tools></div>#}
                </div>
                <div class="ibox-content">
                    {% verbatim %}
                        <table ng-controller="GastronomiaController as gas" ng-table="gas.tableParams" show-filter="true" class="table table-striped table-bordered table-hover">
                           <tr ng-repeat="gastronomia in $data">
                               <td data-title="'Id'" sortable="'id'">{{ gastronomia.id }}</td>
                                <td data-title="'Descripcion'">{{ gastronomia.descripcion }}</td>
                                <td data-title="'Imagen'">{{ gastronomia.imagen }}</td>
                                <td data-title="'Horario'">{{ gastronomia.horario }}</td>
                                <td data-title="'Diashabiles'">{{ gastronomia.diasHabiles }}</td>
                                <td data-title="'Tipo'">{{ gastronomia.tipo }}</td>
                                <td data-title="'Orden'">{{ gastronomia.orden }}</td>
                               <td data-title="'Actions'" >
                                        <ul>
                                            <li>
                                                <a href="">show</a>
                                            </li>
                                            <li>
                                                <a href="">edit</a>
                                            </li>
                                        </ul>
                               </td>
                            </tr>
                        </table>
                    {% endverbatim %}
                </div>
            </div>
        </div>
    </div>
</div>



